{
    "name" : "LamTimer",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": ["storage","tabs"],
    "content_scripts": [
            {
                "matches": ["<all_urls>"],
                "js": ["event.js"]
            }
        ],
    "background" : {
        "scripts" : ["closeTab.js"],
        "persistent" : false
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}